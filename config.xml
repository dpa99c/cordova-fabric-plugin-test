<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.plugin.fabric.test" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Fabric Plugin Test</name>
    <description>
        A Cordova project to test/validate cordova-fabric-plugin
    </description>
    <author email="dave@workingedge.co.uk" href="http://www.workingedge.co.uk">Dave Alden</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <platform name="android">
    </platform>
    <platform name="ios">
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-fabric-plugin" spec="*">
        <variable name="FABRIC_API_KEY" value="1234567890123456789012345678901234567890" />
        <variable name="FABRIC_API_SECRET" value="1234567890123456789012345678901234567890123456789012345678901234" />
    </plugin>
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^6.3.0" />
</widget>
